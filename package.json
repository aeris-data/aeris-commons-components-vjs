{
  "name": "aeris-commons-components-vjs",
  "description": "Aeris commons components. ",
  "version": "0.3.1",
  "author": "Aeris",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.conf --progress",
    "build": "node build/build.js",
    "lint-staged": "lint-staged",
    "format": "node_modules/.bin/eslint --fix \"src/**/*.*(js|vue)\" && node_modules/.bin/stylelint --fix \"src/**/*.vue\"",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "src/**/*.{vue,js}": "node_modules/.bin/eslint"
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "aeris-mixins": "latest",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "vue": "^2.5.2",
    "vue-i18n": "7.0.5",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "@kazupon/vue-i18n-loader": "0.1.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.7.1",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.9.0",
    "style-loader": "^0.18.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0",
    "html-webpack-plugin": "^2.30.1"
  }
}
